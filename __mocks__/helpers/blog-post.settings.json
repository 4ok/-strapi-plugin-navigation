{
  "uid": "application::blog-post.blog-post",
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "Blog post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "searchable": true,
    "previewable": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "altTitle": {
      "type": "string"
    },
    "navigation": {
      "model": "navigationitem",
      "plugin": "navigation",
      "via": "related",
      "configurable": false,
      "hidden": true
    }
  }
}
